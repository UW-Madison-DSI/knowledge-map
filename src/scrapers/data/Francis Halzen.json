        {
            "title": "High-Energy Neutrino Astronomy",
            "digitalObjectIdentifier": "10.1016\/j.nuclphysa.2005.02.032",
            "journalName": "Nuclear Physics A [03759474]",
            "journalVolume": "752",
            "journalIssue": "",
