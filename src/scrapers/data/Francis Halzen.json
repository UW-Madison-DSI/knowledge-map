        {
            "title": "Reconstructing the supernova bounce time with neutrinos in IceCube",
            "digitalObjectIdentifier": "10.1103\/PhysRevD.80.087301",
            "journalName": "Physical Review D [15507998]",
            "journalVolume": "80",
            "journalIssue": "8",
