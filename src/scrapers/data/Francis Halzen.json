        {
            "title": "Minimal cosmogenic neutrinos",
            "digitalObjectIdentifier": "10.1103\/PhysRevD.86.083010",
            "journalName": "Physical Review D [15507998]",
            "journalVolume": "86",
            "journalIssue": "8",
            "firstPage": "",
